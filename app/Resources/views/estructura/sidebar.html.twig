{% import _self as path %}

{% macro check(path) %}
    {% set route = app.request.get('_route') %}
    {% set direct = app.request.pathinfo|split('/')[1] %}
    {{ route in path or path in route or path in direct ? 'active menu-open' : '' }}
{% endmacro check %}

