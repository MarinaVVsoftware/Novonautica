<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>{{ title is defined ? title ~ ' | ' : '' }} Novonautica</title>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <meta name="theme-color" content="#292e41"/>
    <link rel="icon" href="{{ asset('img/favicon.ico') }}">
    <link rel="stylesheet" href="{{ asset('bower_components/bootstrap/dist/css/bootstrap.min.css') }}">
    <link rel="stylesheet" href="{{ asset('bower_components/font-awesome/css/font-awesome.min.css') }}">
    <link rel="stylesheet" href="{{ asset('dist/css/AdminLTE.min.css') }}">
    <link rel="stylesheet" href="{{ asset('dist/css/skins/skin-novo.css') }}">
    <link rel="stylesheet" href="{{ asset('css/main.css') }}">

</head>
<body class="skin-novo sidebar-mini">
<div class="wrapper">
    <header class="main-header">
        <a href="{{ path('inicio') }}" class="logo">
        <span class="logo-mini">
            <img src="{{ asset('img/logo.svg') }}" class="img-responsive center-block">
        </span>
            <span class="logo-lg">
            <img src="{{ asset('img/logo.svg') }}">
        </span>
        </a>
        <nav class="navbar navbar-static-top">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-sm-12">
                        <div class="navbar-custom-menu">
                            <ul class="nav navbar-nav">
                                <li class="dropdown user user-menu">
                                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                        <span class="hidden-xs letra-azul letra-etiqueta-usuario">{{ app.user.username }}</span>
                                    </a>
                                    <ul class="dropdown-menu" style="background-color: transparent; border: 0;">
                                        <li class="user-body" style="background-color: #363c52; border-color: #1c1f29;">
                                            <div class="pull-right">
                                                <a href="{{ path('suppliers_logout') }}" class="btn btn-azul btn-flat">
                                                    <i class="fa fa-power-off" aria-hidden="true"></i>
                                                    Salir
                                                </a>
                                            </div>
                                        </li>
                                    </ul>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </nav>

    </header>
    <div class="content-wrapper" style="margin-left: initial;">
        {% block contwrapper %}{% endblock %}
    </div>
</div>
{% block javascripts %}
    <script src="{{ asset('bower_components/jquery/dist/jquery.min.js') }}"></script>
    <script src="{{ asset('bower_components/bootstrap/dist/js/bootstrap.min.js') }}"></script>
    <script src="{{ asset('dist/js/adminlte.min.js') }}"></script>
    <script src="{{ asset('js/main.js') }}"></script>
{% endblock %}
</body>
</html>
