{% extends 'estructura/plantilla.twig' %}
{% import 'estructura/modalborrar.twig' as modal %}

{% block stylesheets %}
    {{ parent() }}
    <style>
        .timeline > li > .timeline-item {
            border-radius: 0;
            background-color: #292e41;
        }

        .timeline > li > .timeline-item > .timeline-header {
            color: #fff;
            border-bottom-color: #0097f6;
        }

        .timeline > li > .timeline-item > .timeline-body {
            color: #fff;
        }
    </style>
{% endblock %}
{% block contwrapper %}
 <div class="content-wrapper">
     <section class="content">
         <div class="panel panel-novo">
             <div class="panel-heading">
                 <div class="row">
                     <div class="col-sm-6">
                         <h3 class="panel-title">{{ title }}</h3>
                         <span>{{ folio }}</span>
                     </div>
                     <div class="col-sm-6">
                         <a class="btn btn-contorno-azul pull-right" href="{{ path('combustible_index') }}">Regresar</a>
                     </div>
                 </div>
             </div>
             <div class="panel-body panel-pestanias">
                 <ul class="nav nav-tabs" role="tablist">
                     <li role="presentation" class="primero active">
                         <a href="#generales" aria-controls="generales" role="tab" data-toggle="tab">Generales</a>
                     </li>
                     <li role="presentation">
                         <a href="#totales" aria-controls="totales" role="tab" data-toggle="tab">Totales</a>
                     </li>
                     <li role="presentation">
                         <a href="#correo" aria-controls="correo" role="tab" data-toggle="tab">Correo</a>
                     </li>
                     {% if combustible.validacliente == 2 and is_granted(expression('has_role("ROLE_COMBUSTIBLE_PAGO") or (user.isAdmin())')) %}
                         <li role="presentation">
                             <a href="#pagos" aria-controls="pagos" role="tab" data-toggle="tab">Pagos</a>
                         </li>
                     {% endif %}
                     {% if  combustible.cotizacionnotas[0] is defined %}
                         <li role="presentation">
                             <a href="#notas" aria-controls="notas" role="tab" data-toggle="tab">Notas</a>
                         </li>
                     {% endif %}
                     <li role="timeline">
                         <a href="#timeline" aria-controls="timeline" role="tab" data-toggle="tab">Timeline</a>
                     </li>
                 </ul>
                 <div class="tab-content">
                     <div role="tabpanel" class="tab-pane active" id="generales">
                         <div class="row">
                             <div class="col-sm-10 col-sm-offset-1">
                                 <table class="table">
                                     <thead>
                                     <tr>
                                         <th></th>
                                         <th></th>
                                     </tr>
                                     </thead>
                                     <tbody>
                                     <tr>
                                         <th>Folio</th>
                                         <td>{{ folio }}</td>
                                     </tr>
                                     <tr>
                                         <th>Cliente</th>
                                         <td>{{ combustible.cliente }}</td>
                                     </tr>
                                     <tr>
                                         <th>Correo</th>
                                         <td>{{ combustible.cliente.correo }}</td>
                                     </tr>
                                     <tr>
                                         <th>Barco</th>
                                         <td>{{ combustible.barco }}</td>
                                     </tr>
                                     <tr>
                                         <th>Eslora</th>
                                         <td>{{ combustible.barco.eslora }}</td>
                                     </tr>
                                     <tr>
                                         <th>Estatus validación</th>
                                         <td>{{ validacion }}</td>
                                     </tr>
                                     <tr>
                                         <th>Estatus aceptación</th>
                                         <td>{{ aceptacion }}</td>
                                     </tr>
                                     {% if combustible.validacliente == 2 %}
                                         <tr>
                                             <th>Estatus Pago</th>
                                             <td>{{ pago }}</td>
                                         </tr>
                                     {% endif %}
                                     </tbody>
                                 </table>
                             </div>
                         </div>
                         {% if combustible.validanovo == 1 %}
                             <div class="row">
                                 <div class="col-sm-10 col-sm-offset-1">
                                     <label>Observaciones:</label>
                                     <div class="info-input"><p>{{ combustible.notasnovo }}</p></div>
                                 </div>
                             </div>
                         {% endif %}
                         {% if combustible.validacliente == 1 %}
                             <div class="row">
                                 <div class="col-sm-10 col-sm-offset-1">
                                     <label>Observaciones:</label>
                                     <div class="info-input"><p>{{ combustible.notascliente }}</p></div>
                                 </div>
                             </div>
                         {% endif %}
                     </div>
                     <div role="tabpanel" class="tab-pane" id="totales">
                         <div class="row">
                             <div class="col-sm-12 text-right">
                                 <span>Tipo de cambio:</span>
                                 <div>
                                     $ <span id="valdolar">{{ (combustible.dolar/100)|number_format(2) }}</span> <small>MXN</small>
                                 </div>
                             </div>
                         </div>
                         <div class="row">
                             <div class="col-sm-12">
                                 <h2 id="combustible_titulo_mxn" class="text-center">
                                     Abastecimiento de combustible {{ combustible.tipo }}
                                 </h2>
                             </div>
                         </div>
                         <div class="row">
                             <div class="col-sm-3">
                                 <label class="letra-azul tipo-letra2">Precio venta:</label>
                             </div>
                             <div class="col-sm-4">
                                 $ {{ (combustible.precioVenta/100)|number_format(2) }} <small>MXN</small>
                             </div>
                             <div class="col-sm-4">
                                 $ {{ (combustible.precioVenta/combustible.dolar)|number_format(2) }} <small>USD</small>
                             </div>
                         </div>
                         <div class="row">
                             <div class="col-sm-3">
                                 <hr>
                             </div>
                             <div class="col-sm-4">
                                 <hr>
                             </div>
                             <div class="col-sm-4">
                                 <hr>
                             </div>
                         </div>
                         <div class="row">
                             <div class="col-sm-3">
                                 <label class="letra-azul tipo-letra2">Cuota IESPS:</label>
                             </div>
                             <div class="col-sm-4">
                                  {{ combustible.cuotaIesps }}
                             </div>
                         </div>
                         <div class="row">
                             <div class="col-sm-3">
                                 <hr>
                             </div>
                             <div class="col-sm-4">
                                 <hr>
                             </div>
                             <div class="col-sm-4">
                                 <hr>
                             </div>
                         </div>
                         <div class="row">
                             <div class="col-sm-3">
                                 <label class="letra-azul tipo-letra2">Precio sin IESPS:</label>
                             </div>
                             <div class="col-sm-4">
                                 $ {{ (combustible.precioSinIesps/100)|number_format(2) }} <small>MXN</small>
                             </div>
                             <div class="col-sm-4">
                                 $ {{ (combustible.precioSinIesps/combustible.dolar)|number_format(2) }} <small>USD</small>
                             </div>
                         </div>
                         <div class="row">
                             <div class="col-sm-3">
                                 <hr>
                             </div>
                             <div class="col-sm-4">
                                 <hr>
                             </div>
                             <div class="col-sm-4">
                                 <hr>
                             </div>
                         </div>
                         <div class="row">
                             <div class="col-sm-3">
                                 <label class="letra-azul tipo-letra2">Precio sin IVA/IESPS:</label>
                             </div>
                             <div class="col-sm-4">
                                 $ {{ (combustible.precioSinIvaIesps/100)|number_format(2) }} <small>MXN</small>
                             </div>
                             <div class="col-sm-4">
                                 $ {{ (combustible.precioSinIvaIesps/combustible.dolar)|number_format(2) }} <small>USD</small>
                             </div>
                         </div>
                         <div class="row">
                             <div class="col-sm-3">
                                 <hr>
                             </div>
                             <div class="col-sm-4">
                                 <hr>
                             </div>
                             <div class="col-sm-4">
                                 <hr>
                             </div>
                         </div>
                         <div class="row">
                             <div class="col-sm-3">
                                 <label class="letra-azul tipo-letra2">Litros vendidos:</label>
                             </div>
                             <div class="col-sm-8">
                                 <span class="pl-1">{{ combustible.cantidad }}</span>
                             </div>
                         </div>
                         <div class="row">
                             <div class="col-sm-3">
                                 <hr>
                             </div>
                             <div class="col-sm-8">
                                 <hr>
                             </div>
                         </div>
                         <div class="row">
                             <div class="col-sm-3">
                                 <label class="letra-azul tipo-letra2">IESPS:</label>
                             </div>
                             <div class="col-sm-4">
                                 $ {{ (combustible.iespsTotal/100)|number_format(2) }} <small>MXN</small>
                             </div>
                             <div class="col-sm-4">
                                 $ {{ (combustible.iespsTotal/combustible.dolar)|number_format(2) }} <small>USD</small>
                             </div>
                         </div>
                         <div class="row">
                             <div class="col-sm-3">
                                 <hr>
                             </div>
                             <div class="col-sm-4">
                                 <hr>
                             </div>
                             <div class="col-sm-4">
                                 <hr>
                             </div>
                         </div>
                         <div class="row">
                             <div class="col-sm-3">
                                 <label class="letra-azul tipo-letra2">Total sin IESPS:</label>
                             </div>
                             <div class="col-sm-4">
                                 $ {{ (combustible.totalSinIesps/100)|number_format(2) }} <small>MXN</small>
                             </div>
                             <div class="col-sm-4">
                                 $ {{ (combustible.totalSinIesps/combustible.dolar)|number_format(2) }} <small>USD</small>
                             </div>
                         </div>
                         <div class="row">
                             <div class="col-sm-3">
                                 <hr>
                             </div>
                             <div class="col-sm-4">
                                 <hr>
                             </div>
                             <div class="col-sm-4">
                                 <hr>
                             </div>
                         </div>
                         <div class="row">
                             <div class="col-sm-3">
                                 <label class="letra-azul tipo-letra2">Tasa IVA:</label>
                             </div>
                             <div class="col-sm-8">
                                 <span class="pl-1">{{ (combustible.iva + 100)/100 }}</span>
                             </div>
                         </div>
                         <div class="row">
                             <div class="col-sm-3">
                                 <hr>
                             </div>
                             <div class="col-sm-8">
                                 <hr>
                             </div>
                         </div>
                         <div class="row">
                             <div class="col-sm-3">
                                 <label class="letra-azul tipo-letra2">Total sin IVA/IESPS:</label>
                             </div>
                             <div class="col-sm-4">
                                 $ {{ (combustible.totalSinIvaIesps/100)|number_format(2) }} <small>MXN</small>
                             </div>
                             <div class="col-sm-4">
                                 $ {{ (combustible.totalSinIvaIesps/combustible.dolar)|number_format(2) }} <small>USD</small>
                             </div>
                         </div>
                         <div class="row">
                             <div class="col-sm-3">
                                 <hr>
                             </div>
                             <div class="col-sm-4">
                                 <hr>
                             </div>
                             <div class="col-sm-4">
                                 <hr>
                             </div>
                         </div>
                         <div class="row">
                             <div class="col-sm-3">
                                 <label class="letra-azul tipo-letra2">Subtotal:</label>
                             </div>
                             <div class="col-sm-4">
                                 $ {{ (combustible.subtotal/100)|number_format(2) }} <small>MXN</small>
                             </div>
                             <div class="col-sm-4">
                                 $ {{ (combustible.subtotal/combustible.dolar)|number_format(2) }} <small>USD</small>
                             </div>
                         </div>
                         <div class="row">
                             <div class="col-sm-3">
                                 <hr>
                             </div>
                             <div class="col-sm-4">
                                 <hr>
                             </div>
                             <div class="col-sm-4">
                                 <hr>
                             </div>
                         </div>
                         <div class="row">
                             <div class="col-sm-3">
                                 <label class="letra-azul tipo-letra2">IVA:</label>
                             </div>
                             <div class="col-sm-4">
                                 $ {{ (combustible.ivaTotal/100)|number_format(2) }} <small>MXN</small>
                             </div>
                             <div class="col-sm-4">
                                 $ {{ (combustible.ivaTotal/combustible.dolar)|number_format(2) }} <small>USD</small>
                             </div>
                         </div>
                         <div class="row">
                             <div class="col-sm-3">
                                 <hr>
                             </div>
                             <div class="col-sm-4">
                                 <hr>
                             </div>
                             <div class="col-sm-4">
                                 <hr>
                             </div>
                         </div>
                         <div class="row">
                             <div class="col-sm-3">
                                 <label class="letra-azul tipo-letra2">Total:</label>
                             </div>
                             <div class="col-sm-4">
                                 $ {{ (combustible.total/100)|number_format(2) }} <small>MXN</small>
                             </div>
                             <div class="col-sm-4">
                                 $ {{ (combustible.total/combustible.dolar)|number_format(2) }} <small>USD</small>
                             </div>
                         </div>
                         <div class="row">
                             <div class="col-sm-3">
                                 <hr>
                             </div>
                             <div class="col-sm-4">
                                 <hr>
                             </div>
                             <div class="col-sm-4">
                                 <hr>
                             </div>
                         </div>
                     </div>
                     <div role="tabpanel" class="tab-pane" id="correo">
                         <div class="row">
                             <div class="col-sm-12">
                                 <label>Mensaje en el correo:</label>
                                 <div class="info-input">{{ combustible.mensaje|raw }}</div>
                             </div>
                         </div>
                     </div>
                     {% if combustible.validacliente == 2 and is_granted(expression('has_role("ROLE_COMBUSTIBLE_PAGO") or (user.isAdmin())')) %}
                         <div role="tabpanel" class="tab-pane" id="pagos">
                             <div class="row">
                                 <div class="col-sm-3">
                                     <label>Total a pagar</label>
                                     <div class="info-input">
                                         $ {{ (combustible.total/100)|number_format(2) }} MXN
                                     </div>
                                 </div>
                             </div>
                             <div class="row espacio-linea">
                                 <div class="col-sm-6">
                                     <hr>
                                 </div>
                             </div>
                             <div class="row">
                                 <div class="col-sm-12">
                                     <table class="table text-center">
                                         <thead>
                                         <tr>
                                             <th>Método de pago</th>
                                             <th>Pago</th>
                                             <th>Fecha</th>
                                             <th>Tipo de cambio</th>
                                             <th>Total en MXN</th>
                                             <th>Factura</th>
                                         </tr>
                                         </thead>
                                         <tbody>
                                         {% set totUsd = 0 %}
                                         {% for pago in combustible.pagos %}
                                             {% set factura = pago.factura %}
                                             <tr>
                                                 <td>{{ pago.metodopago }}</td>
                                                 <td>$ {{ (pago.cantidad/100)|number_format(2) }} MXN</td>
                                                 <td>{% if pago.fechaRealPago %}
                                                         {{ pago.fechaRealPago|date('d-m-Y') }}
                                                     {% endif %}</td>
                                                 <td>$ {{ (pago.dolar/100)|number_format(2) }} MXN</td>
                                                 <td>$ {{ (pago.cantidad / pago.dolar)|number_format(2) }} USD</td>
                                                 <td>
                                                     {% if factura %}
                                                         <a href="{{ path('contabilidad_factura_pdf', { 'id': factura.id }) }}"
                                                            target="_blank"
                                                            class="remove-motor btn btn-xs btn-azul"
                                                            style="margin: 5px;"
                                                         >
                                                             PDF
                                                         </a>
                                                     {% endif %}
                                                 </td>
                                             </tr>
                                             {% set totUsd = totUsd + (pago.cantidad / pago.dolar) %}
                                         {% endfor %}
                                         </tbody>
                                     </table>
                                 </div>
                             </div>
                             <div class="row espacio-linea">
                                 <div class="col-sm-6">
                                     <hr>
                                 </div>
                             </div>
                             <div class="row">
                                 <div class="col-sm-3">
                                     <label>Pagado hasta el momento</label>
                                     <div class="info-input">
                                         $ {{ (combustible.pagado/100)|number_format(2) }} MXN
                                     </div>
                                 </div>
                                 <div class="col-sm-3">
                                     <label></label>
                                     <div class="info-input">
                                         $ {{ (totUsd)|number_format(2) }} USD
                                     </div>
                                 </div>
                             </div>
                             <div class="row espacio-linea-arriba2">
                                 <div class="col-sm-3">
                                     <label>Saldo faltante</label>
                                     <div class="info-input">
                                         $ {{ ((combustible.total-combustible.pagado)/100)|number_format(2) }}
                                         MXN
                                     </div>
                                 </div>
                             </div>
                         </div>
                     {% endif %}
                     {% if combustible.cotizacionnotas[0] is defined %}
                         <div role="tabpanel" class="tab-pane" id="notas">
                             <div class="row">
                                 <div class="col-sm-12">
                                     <ul class="timeline sin-linea">
                                         {% for nota in combustible.cotizacionnotas %}
                                             <li>
                                                 <div class="timeline-item">
                                                     <span class="time">{{ nota.fechahoraregistro|date('d M. Y H:s') }}</span>
                                                     <h3 class="timeline-header">{{ nota.descripcion }}</h3>
                                                 </div>
                                             </li>
                                         {% endfor %}
                                     </ul>
                                 </div>
                             </div>
                         </div>
                     {% endif %}
                     <div role="tabpanel" class="tab-pane" id="timeline">

                         <ul class="timeline">
                             <li class="time-label">
                                 <span class="bg-blue">{{ combustible.fecha|date('d M. Y') }}</span>
                             </li>
                             <li>
                                 <i class="fa fa-file"></i>
                                 <div class="timeline-item">
                                     <h3 class="timeline-header">Se creo la cotizacion Folio: #{{ folio }}</h3>
                                 </div>
                             </li>
                             {% if combustible.validanovo != 0 %}
                                 {% if combustible.validanovo == 2 %}
                                     <li>
                                         <i class="fa fa-check-circle bg-green"></i>
                                         <div class="timeline-item">
                                            <span class="time">
                                                {{ combustible.registroValidaNovo ? combustible.registroValidaNovo|date('d M. Y H:s') : '' }}
                                            </span>
                                             <h3 class="timeline-header">Aceptada por novonautica</h3>
                                         </div>
                                     </li>
                                 {% else %}
                                     <li>
                                         <i class="fa fa-times bg-red"></i>
                                         <div class="timeline-item">
                                            <span class="time">
                                                {{ combustible.registroValidaNovo ? combustible.registroValidaNovo|date('d M. Y H:s') : '' }}
                                            </span>
                                             <h3 class="timeline-header">Rechazada por novonautica</h3>
                                             {% if combustible.notasnovo %}
                                                 <div class="timeline-body">
                                                     {{ combustible.notasnovo }}
                                                 </div>
                                             {% endif %}
                                         </div>
                                     </li>
                                 {% endif %}
                             {% endif %}
                             {% if combustible.validacliente != 0 %}
                                 {% if combustible.validacliente == 2 %}
                                     <li>
                                         <i class="fa fa-check-square bg-green"></i>
                                         <div class="timeline-item">
                                                <span class="time">
                                                    {{ combustible.registroValidaCliente ? combustible.registroValidaCliente|date('d M. Y H:s') : '' }}
                                                </span>
                                             <h3 class="timeline-header">Aceptada por el cliente</h3>
                                         </div>
                                     </li>
                                 {% else %}
                                     <li>
                                         <i class="fa fa-times bg-red"></i>
                                         <div class="timeline-item">
                                                <span class="time">
                                                    {{ combustible.registroValidaCliente ? combustible.registroValidaCliente|date('d M. Y H:s') : '' }}
                                                </span>
                                             <h3 class="timeline-header">Rechazada por el cliente</h3>
                                             {% if combustible.notascliente %}
                                                 <div class="timeline-body">
                                                     {{ combustible.notascliente }}
                                                 </div>
                                             {% endif %}
                                         </div>
                                     </li>
                                 {% endif %}
                             {% endif %}
                             {% if combustible.estatuspago != 0 %}
                                 {% set total = combustible.total %}
                                 {% set pagado = 0 %}
                                 <li>
                                     <i class="fa fa-usd bg-yellow"></i>
                                     <div class="timeline-item">
                                            <span class="time">
                                                {{ combustible.fecha ? combustible.fecha|date('d M. Y H:s') : '' }}
                                            </span>
                                         <h3 class="timeline-header">Nuevo saldo pendiente</h3>
                                         <div class="timeline-body">
                                             $ {{ (combustible.total / 100)|number_format(2) }}
                                             MXN
                                         </div>
                                     </div>
                                 </li>
                                 {% for pago in combustible.pagos %}
                                     {% set pagado = pagado + pago.cantidad %}
                                     <li>
                                         <i class="fa fa-envelope bg-green"></i>
                                         <div class="timeline-item">
                                                <span class="time">
                                                    {{ pago.fechaRegistro ? pago.fechaRegistro|date('d M. Y H:s') : '' }}
                                                </span>
                                             <h3 class="timeline-header">Nuevo abono</h3>
                                             <div class="timeline-body">
                                                 ${{ (pago.cantidad / 100)|number_format(2) }} MXN
                                             </div>
                                         </div>
                                     </li>
                                     {% if pago.factura %}
                                         <li>
                                             <i class="fa fa-file-pdf-o bg-blue"></i>
                                             <div class="timeline-item">
                                                <span class="time">
                                                    {{ pago.factura.fecha|date('d M. Y H:s') }}
                                                </span>
                                                 <h3 class="timeline-header">Facturado</h3>
                                             </div>
                                         </li>
                                     {% endif %}
                                 {% endfor %}
                                 {% if combustible.estatuspago == 2 %}
                                     <li>
                                         <i class="fa fa-dollar bg-green"></i>
                                         <div class="timeline-item">
                                             <h3 class="timeline-header">Pagado</h3>
                                         </div>
                                     </li>

                                     <li class="time-label">
                                            <span class="bg-blue">
                                                {{ combustible.registroPagoCompletado ? combustible.registroPagoCompletado|date('d M. Y') : '' }}
                                            </span>
                                     </li>
                                 {% endif %}
                             {% endif %}
                         </ul>
                     </div>
                 </div>
                 <div class="row">
                     <div class="col-sm-12">
                         <a target="_blank" class="btn btn-morado-claro pull-right"
                            href="{{ path('combustible_pdf',{'id': combustible.id }) }}">
                             <i class="fa fa-file-pdf-o" aria-hidden="true"></i>
                             Ver
                         </a>
                     </div>
                 </div>
                 {% if combustible.validanovo == 0 and is_granted(expression('has_role("COMBUSTIBLE_COTIZACION_VALIDATE") or (user.isAdmin())')) %}
                     <div class="row espacio-linea-arriba2">
                         <div class="col-sm-12">
                             <a class="btn btn-azul pull-right"
                                href="{{ path('combustible_validar', { 'id': combustible.id }) }}">
                                 <i class="fa fa-pencil-square-o" aria-hidden="true"></i>
                                 Validar
                             </a>
                         </div>
                     </div>
                 {% endif %}
                 {% if combustible.validanovo == 2 and combustible.validacliente == 0 and is_granted(expression('has_role("COMBUSTIBLE_COTIZACION_VALIDATE") or (user.isAdmin())')) %}
                     <div class="row espacio-linea-arriba2">
                         <div class="col-sm-12">
                             <a class="btn btn-azul pull-right"
                                href="{{ path('combustible_validar', { 'id': combustible.id }) }}">
                                 <i class="fa fa-pencil-square-o" aria-hidden="true"></i>
                                 Aceptar Cliente
                             </a>
                         </div>
                     </div>
                 {% endif %}
                 {% if (combustible.validanovo == 1 or
                     combustible.validacliente == 1) and
                     combustible.estatus == 1 and
                     is_granted(expression('has_role("COMBUSTIBLE_COTIZACION_REQUOTE") or (user.isAdmin())')) %}
                     <div class="row espacio-linea-arriba2">
                         <div class="col-sm-12">
                             <a class="btn btn-naranja pull-right"
                                href="{{ path('combustible_recotizar', { 'id': combustible.id }) }}">
                                 <i class="fa fa-pencil-square" aria-hidden="true"></i>
                                 Recotizar
                             </a>
                         </div>
                     </div>
                 {% endif %}
                 {% if combustible.validacliente == 2 and
                     combustible.estatuspago != 2 and
                     is_granted('ROLE_COMBUSTIBLE_PAGO') %}
                     <div class="row espacio-linea-arriba2">
                         <div class="col-sm-12">
                             <a class="btn btn-verde pull-right"
                                href="{{ path('combustible_pago_edit',{'id': combustible.id}) }}">
                                 <i class="fa fa-plus-square-o" aria-hidden="true"></i>
                                 Pagos
                             </a>
                         </div>
                     </div>
                 {% endif %}
                 {% if combustible.validanovo == 2 %}
                     <div class="row espacio-linea-arriba2">
                         <div class="col-sm-12">
                             <button class="btn btn-naranja pull-right loadpage" id="reenviar"
                                     data-idcotizacion="{{ combustible.id }}">
                                 <i class="fa fa-paper-plane-o" aria-hidden="true"></i>
                                 Reenviar
                             </button>
                         </div>
                     </div>
                 {% endif %}
                 <div class="row espacio-linea-arriba2">
                     <div class="col-sm-12">
                         <a class="btn btn-contorno-azul pull-right"
                            href="{{ path('combustible_nota',{'id': combustible.id}) }}">
                             <i class="fa fa-plus" aria-hidden="true"></i>
                             Nota
                         </a>
                     </div>
                 </div>
                 {% if combustible.validanovo == 0 and
                     is_granted(expression('has_role("COMBUSTIBLE_COTIZACION_DELETE") or (user.isAdmin())')) %}
                     <div class="row espacio-linea-arriba2">
                         <div class="col-sm-12">
                             {{ delete_form is defined ? modal.deleteButton('Cotización') : '' }}
                         </div>
                     </div>
                 {% endif %}
             </div>
         </div>
     </section>
 </div>
{% endblock %}
{% block javascripts %}
    {{ parent() }}
    <script type="text/javascript">
        $('#reenviar').on('click', function () {
            var url = "{{ path('combustible_reenviar',{'id':'idcotizacion'}) }}";
            url = url.replace("idcotizacion", $(this).data('idcotizacion'));
            $.ajax({
                method: "POST",
                url: url,
                success: function () {
                    $('#loading').hide();
                    $('#estatusEnvio').modal('show');
                    setTimeout(function () {
                        $('#estatusEnvio').modal('hide');
                    }, 3000);
                }
            });
        });
    </script>
{% endblock %}
{% block modals %}
    <div id="estatusEnvio" class="modal fade ventanaborrar" tabindex="-1" role="dialog">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span
                                aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title">Mensaje</h4>
                </div>
                <div class="modal-body">
                    <p>Correo enviado con éxito</p>
                </div>
                <div class="modal-footer">

                </div>
            </div><!-- /.modal-content -->
        </div><!-- /.modal-dialog -->
    </div><!-- /.modal -->
    {{ delete_form is defined ? modal.deleteModal(delete_form) : '' }}
{% endblock %}