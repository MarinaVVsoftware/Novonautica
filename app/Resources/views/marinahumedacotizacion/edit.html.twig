{% extends 'estructura/plantilla.twig' %}

{% block title %}
    Novonautica | Editar cotizaci√≥n Marina Humeda
{% endblock %}

{% block contwrapper %}
<div class="content-wrapper">
    <section class="content">
        <div class="panel panel-novo">
            <div class="panel-heading">
                <div class="row">
                    <div class="col-sm-6">
                        <h3 class="panel-title">Marina Humeda Editar</h3>
                    </div>
                    <div class="col-sm-6">
                        <a class="btn btn-contorno-azul pull-right" href="{{ path('marina-humeda_index') }}">Regresar</a>
                    </div>
                </div>

            </div>
            <div class="panel-body">
                {{ form_start(edit_form) }}
                {{ form_widget(edit_form) }}
                <div class="row">
                    <div class="col-sm-12">
                        <input type="submit" value="Guardar" class="btn btn-azul center-block" />
                    </div>
                </div>
                {{ form_end(edit_form) }}
                <div class="row">
                    <div class="col-sm-12">
                        {{ form_start(delete_form) }}
                        <input type="submit" value="Borrar" class="btn btn-borrar pull-right">
                        {{ form_end(delete_form) }}
                    </div>
                </div>
            </div>
        </div>
    </section>
</div>
{% endblock %}
